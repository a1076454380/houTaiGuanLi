	<script src="../js/jquery.paginate.js" type="text/javascript" charset="utf-8"></script>
	<script type="text/javascript">
		$(function() {
			$('.file-click').click(function(event) {
				if($(this).next().attr('class') == 'file-wrap') {
					$(this).next().addClass('block')
				} else if($(this).next().attr('class') != undefined) {
					$(this).next().removeClass('block')
				}
			})
//			遮蒙层
			$('.delete').click(function(){
				$('.wjdelete').show()
			})
			
			$('.downn').click(function(){
				$('.down').show()
			})
			$('.num').click(function(){
				$(this).siblings().removeClass('active01')
				$(this).addClass('active01')
			})
            $("#demo5").paginate({
                count: 4,
                start: 1,
                display: 7,
                border: false,
                border_color: '#fff',
                text_color: 'white',
                background_color: '#FFB264',
                border_hover_color: '#ccc',
                text_hover_color: '#FFB264',
                background_hover_color: '#fff',
                images: false,
                mouse: 'press',
                onChange: function (pageNum) {
                    console.log(pageNum);
                    $('._current', '#paginationdemo').removeClass('_current').hide();
                    $('#p' + pageNum).addClass('_current').show();
                }
            });
            $('.jPag-first').html('首页')
            $('.jPag-last').html('末页')
		})
	</script>

