	<script type="text/javascript">
		$(function(){
			$('#power-button').click(function(){
				$('.rgba').show()
			})
			$('.rgba').click(function(){
				$('.rgba').hide()
			})
		//	新增权限其他显示
			$('.newmain .other').click(function(){
				if($('.newmain .other-group').css('display')=='none'){
					$('.newmain .other-group').show()
					$(this).css('color','#333')
				}else{
					$('.newmain .other-group').hide()
					$(this).css('color','#d45e12')
				}
			})
			//	权限更改
			$('.mypower .power-change').click(function(){
				
				if($(this).next().attr('class')=='power-state no'){
					$(this).next().attr('class','power-state no')
				}else{
					$(this).next().attr('class','power-state')
				}
			})
//			权限设定对象点击效果
			$('.new-group').click(function(){
				$('.new-group').removeClass('active')
				$(this).addClass('active')
			})
		})
